{
  "extends": "airbnb-base",
  "plugins": ["cypress"],
  "env": {
    "browser": true,
    "es6": true,
    "cypress/globals": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": "error",
    "arrow-parens": ["error", "as-needed"],
    "no-param-reassign": "off"
  }
}
