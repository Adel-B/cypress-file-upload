  "description": "",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "parcel build src/index.html",
    "deploy": "VERCEL_ORG_ID=${VERCEL_ORG_ID} VERCEL_PROJECT_ID=${REACT_HTML_INPUT_VERCEL_PROJECT_ID} vercel -t ${VERCEL_TOKEN} -c deploy --prod",
    "dev": "parcel src/index.html --port=3000",
    "start": "npm run dev",
    "lint": "eslint src cypress --ext=js,jsx",
    "test": "cypress run",
    "test:ci": "npm test -- --config-file cypress.ci.json",
    "test:open": "cypress open"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
